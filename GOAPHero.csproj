<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>


    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>ConsoleHero</PackageId>
    <Version>0.1.0</Version>
    <Authors>Derek Gooding</Authors>
    <Description>A library for making GOAP systems in C#.</Description>
    <PackageTags>GOAP</PackageTags>

    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/DerekGooding/GOAPHero</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DerekGooding/GOAPHero</RepositoryUrl>
    <PackageBugTrackerUrl>https://github.com/DerekGooding/GOAPHero/issues</PackageBugTrackerUrl>
    <RepositoryType>git</RepositoryType>	

  </PropertyGroup>
</Project>
